<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="255.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controller.inventory.ConfirmExportCtrl">
   <children>
      <VBox layoutX="85.0" layoutY="23.0" prefHeight="178.0" prefWidth="258.0" spacing="10.0">
         <children>
            <TextField fx:id="total_items" editable="false" />
            <TextField fx:id="location" editable="false" />
            <TextField fx:id="reference" editable="false" />
            <TextField fx:id="user" editable="false" />
            <TextField fx:id="date_time" editable="false" />
         </children>
      </VBox>
      <Label layoutX="17.0" layoutY="28.0" text="Total Items" />
      <Label layoutX="30.0" layoutY="64.0" prefHeight="17.0" prefWidth="53.0" text="Location" />
      <Label layoutX="23.0" layoutY="98.0" text="Reference" />
      <Label layoutX="51.0" layoutY="133.0" text="User" />
      <Button layoutX="279.0" layoutY="214.0" mnemonicParsing="false" onAction="#onConfirm" text="Confirm">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button layoutX="209.0" layoutY="214.0" mnemonicParsing="false" onAction="#onCancel" prefHeight="25.0" prefWidth="62.0" text="Cancel" />
